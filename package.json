{
  "name": "DnD-Character-Factory",
  "version": "1.0.0",
  "private": "true",
  "description": "Generate and track Dungeons and Dragons characters",
  "main": "app.js",
  "scripts": {
    "bundle": "jspm bundle app/main.ts public/bundle.js --minify --inject",
    "dev": "nodemon app.js",
    "init_db": "tools/cmd/mongo_import_tables",
    "init_table": "tools/cmd/mongo_import_table",
    "unbundle": "jspm unbundle"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CarterNelms/DnD-Character-Factory.git"
  },
  "author": "Carter Nelms",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CarterNelms/DnD-Character-Factory/issues"
  },
  "homepage": "https://github.com/CarterNelms/DnD-Character-Factory",
  "dependencies": {
    "express": "^4.13.4",
    "jade": "^1.11.0",
    "less": "^2.7.1",
    "lodash": "^4.13.1",
    "mongodb": "^2.1.21",
    "mongoose": "^4.4.20"
  },
  "devDependencies": {
    "grunt": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-jade": "^1.0.0",
    "grunt-contrib-jshint": "^1.0.0",
    "grunt-contrib-less": "^1.3.0",
    "grunt-contrib-watch": "^1.0.0",
    "jshint": "^2.9.2",
    "jspm": "^0.16.39",
    "nodemon": "^1.9.2"
  },
  "jspm": {
    "directories": {
      "baseURL": "public"
    },
    "dependencies": {
      "angular2": "npm:angular2@^2.0.0-beta.17",
      "reflect-metadata": "npm:reflect-metadata@0.1.2",
      "rxjs": "npm:rxjs@5.0.0-beta.9",
      "ts": "github:frankwallis/plugin-typescript@^4.0.16",
      "zone.js": "npm:zone.js@0.6.12"
    },
    "devDependencies": {
      "typescript": "npm:typescript@^1.6.2"
    }
  }
}
